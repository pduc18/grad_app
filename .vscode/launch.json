{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32 (ST-Link)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "stlink",
      "cwd": "${workspaceFolder}",
      "executable": "build/MyProject.elf",
      "device": "STM32F103C8",
      "interface": "swd",
      "runToEntryPoint": "main",
      "svdFile": "${workspaceFolder}/STM32F103xx.svd",
      "preLaunchTask": "Build Project"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/duc/Documents/grad_prj/grad_app_log_test/grad_app/Core/Src",
      "program": "/home/duc/Documents/grad_prj/grad_app_log_test/grad_app/Core/Src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}